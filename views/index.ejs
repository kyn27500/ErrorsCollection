<!DOCTYPE html>
<html>
  <head>
    <title>错误列表</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>错误列表</h1>
    <!-- table -->

    <table id="tbody" border="1" width="80%" align="center" style="table-layout: fixed">
    	<tr>
    		<td width="15%">游戏ID</td>
    		<td width="15%">平台</td>
    		<td width="20%">时间</td>
    		<td>错误</td>
    	</tr>
    </table>
  </body>


  <script type="text/javascript">
    	function drawTable(list){  
		      
		    for(var i=0;i<list.length;i++){  
				var tr=document.createElement('tr');  
				var td_gameid=document.createElement('td')  
				var td_platform=document.createElement('td')  
				var td_time=document.createElement('td') 
				var td_error=document.createElement('td') 

				td_gameid.innerHTML=list[i].gameid;  
				td_platform.innerHTML=list[i].platform;  
				td_time.innerHTML=list[i].time;  
				td_error.innerHTML = list[i].error;
				tr.appendChild(td_gameid);  
				tr.appendChild(td_platform);  
				tr.appendChild(td_time); 
				tr.appendChild(td_error);  

				var tbody=document.getElementById('tbody');  
				tbody.appendChild(tr);  
		    }  
		} 


		var data = [{"id":9,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":10,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":11,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":12,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":13,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":14,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":15,"gameid":5,"platform":"231","error":"131231","time":121412419},{"id":16,"gameid":0,"platform":"unknown","error":"Test data","time":1490885641322},{"id":17,"gameid":5,"platform":"231","error":"","time":121412419},{"id":18,"gameid":0,"platform":"unknown","error":"Test data","time":1490966187895}]

		drawTable(data);
    </script>

</html>
